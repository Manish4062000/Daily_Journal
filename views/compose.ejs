<%- include("partials/header"); %>
<h1>Compose</h1>
<form class="data" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control"  name="postTitle" autocomplete="off">
    <label>Post</label>
    <textarea  name="postBody" class="form-control" rows="5"></textarea>
  </div>
  <button  class ="btn btn-primary buttonRadius" type="submit" name="button" >Publish</button>
</form>

<%- include("partials/footer"); %>
